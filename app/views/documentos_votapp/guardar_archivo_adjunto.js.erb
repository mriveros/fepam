<% if @guardado_ok == true %>

	$("#form_buscar_documentos_fepam_id").val('<%= @documento_fepam.id %>');
  	$("#form_buscar_documentos_fepam_numero").val('');
  	$("#form-buscar-documentos-fepam").submit();


  setTimeout(function(){

    $("#<%= params[:window_id]%>").modal('hide'); 
   
    swal("Archivo Adjunto Exitosamente.", "", "success");

  }, 1000);
  

<% else %>
  
  swal({title: "Atención", text: " No se pudo adjuntar el archivo. Intente más tarde. <%= @msg %>", html: false});

<% end %>
