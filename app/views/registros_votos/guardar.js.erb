<% if @registro_voto_ok %>

  $("<div class='col-sm-12' id='msg-agregar-registro-voto' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-registro-voto");
   
  $("#form_buscar_registros_votos_id").val('<%= @registro_voto.id %>');
  $("#form_buscar_registros_votos_fecha_registro").val('');
  $("#form_buscar_registros_votos_cedula").val('');
  $("#form_buscar_registros_votos_nombre").val('');
  $("#form_buscar_registros_votos_apellido").val('');
  $("#form_buscar_registros_votos_partido").val('');
  
  swal("Guardado Exitosamente.")
  
  setTimeout(function(){ 
    $("#msg-agregar-registro-voto").remove(); 
  	$("#<%= params[:window_id]%>").modal('hide'); 
  	$("#form-buscar-registros-votos").submit(); 

  }, 1500);


<% else %>

    $("<div class='col-sm-12' id='msg-agregar-registro-voto' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-agregar-registro-voto");
    swal("<%=@msg%>")
    setTimeout(function(){ 
      $("#msg-agregar-registro-voto").remove(); 
    }, 2500);
<% end %>
