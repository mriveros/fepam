<% if @votante_ok %>

   $("<div class='col-sm-12' id='msg-agregar-votante' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-votante");
   
  $("#form_buscar_votantes_id").val('<%= @votante.id %>');
  $("#form_buscar_votantes_descripcion").val('');
  $("#form_buscar_votantes_sueldo").val('');
  
  swal("Guardado Exitosamente.")
  
  setTimeout(function(){ 

  	$("#<%= params[:window_id]%>").modal('hide'); 
  	$("#form-buscar-votantes").submit(); 

  }, 1500);


<% else %>

    $("<div class='col-sm-12' id='msg-agregar-votante' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-agregar-votante");

<% end %>
