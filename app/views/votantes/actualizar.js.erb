<% if @votante_ok.present? %>

   $("<div class='col-sm-12' id='msg-editar-votante' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Actualizado Exitosamente.</div>").insertBefore("#form-editar-votante");
   
  $("#form_buscar_votantes_id").val('<%= @votante.id %>');
  $("#form_buscar_votantes_cedula").val('');
  $("#form_buscar_votantes_nombre").val('');
  $("#form_buscar_votantes_apellido").val('');
  $("#form_buscar_votantes_direccion").val('');
  $("#form_buscar_votantes_partido").val('');

  setTimeout(function(){

  	swal("Actualizado Exitosamente.")
  	$("#<%= params[:window_id]%>").modal('hide'); 
  	$("#form-buscar-votantes").submit(); 

  }, 1500);

<% else %>

  $("<div class='col-sm-12' id='msg-editar-votante' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'>Error al modificar el votante. Intente m√°s tarde.</div>").insertBefore("#form-editar-votante");

<% end %>
