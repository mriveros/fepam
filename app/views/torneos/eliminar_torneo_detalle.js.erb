<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-torneo-detalle' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-torneo-detalle");
   
    setTimeout(

      function(){ $("#msg-eliminar-torneo-detalle").remove(); 
     
      $("#link-to-torneo-detalle-<%= params[:torneo_id]%>").trigger('click');
      $("#link-to-torneo-detalle-<%= params[:torneo_id]%>").trigger('click');
      
       swal("Fecha eliminada exitosamente.");

    }, 2000);
   
    
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-torneo-detalle' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg).html_safe %></div>").insertBefore("#table-torneo-detalle");
  setTimeout(function(){ 
    
    $("#msg-eliminar-torneo-detalle").remove();

    swal("No se pudo eliminar la fecha. Intente m√°s tarde.")

     }, 2000);
    
<% end %>