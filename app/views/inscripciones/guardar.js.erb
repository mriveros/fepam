<% if @inscripcion_ok %>

   $("<div class='col-sm-12' id='msg-agregar-inscripcion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-inscripcion");
   
  $("#form_buscar_inscripciones_id").val('<%= @inscripcion.id %>');
  $("#form_buscar_inscripciones_torneo_id").val('');
  $("#form_buscar_inscripciones_torneo_detalle_id").val('');
  $("#form_buscar_inscripcions_categoria_id").val('');
  $("#form_buscar_inscripciones_fecha_inicio_inscripcion").val('');
  
  swal("Inscripci√≥n habilitada exitosamente.")
  
  setTimeout(function(){ 

  	$("#<%= params[:window_id]%>").modal('hide'); 
  	$("#form-buscar-inscripciones").submit(); 

  }, 1500);
 

<% else %>

    $("<div class='col-sm-12' id='msg-agregar-inscripcion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-agregar-inscripcion");
    swal("<%=@msg%>");
    
<% end %>
